proc ZV601(INT BROJ,INT JJ)

def int prol,vrol
def string[80] poruka

if (bfazaosc[jj]==2)
  prol=brojpocetka[bfazaosc[jj]]
  vrol=0
else
  prol=brojpocetka[bfazaosc[jj]]-brojpocetka[bfazaosc[jj]+1]
  vrol=brojpocetka[bfazaosc[jj]+1]
endif
poruka=BPORUKA[BFAZAOSC[JJ]]<<"-"<<(broj+1)
poruka=poruka << " $66015-" << jj-vrol+1 << "/" << prol
poruka=poruka<<" $66017-"<<jj+1<< "/" << (BBROJkoraka)

msg( poruka )

M17

